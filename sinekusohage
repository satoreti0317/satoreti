local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- Ë®±ÂèØ„Åï„Çå„Åü„Ç≠„Éº‰∏ÄË¶ß
local ALLOWED_KEYS = {
    [7056148213] = "satoreti",
    [7239395171] = "satoreti",
    [6092005701] = "tamago",
    [7258182195] = "satoreti",
    [5855902831] = "pink",
    [7330893561] = "satoreti",
    [5826682471] = "hage",
    [4159185207] = "hage",
}

-- Êú™Ë®±ÂèØ„Å™„Çâ„Ç≠„ÉÉ„ÇØ
if not ALLOWED_KEYS[LocalPlayer.UserId] then
    local StarterGui = game:GetService("StarterGui")
    StarterGui:SetCore("SendNotification", {
        Title = "ÊãíÂê¶",
        Text = "„Ç¢„ÇØ„Çª„Çπ„ÅåÊãíÂê¶„Åï„Çå„Åæ„Åó„Åü\n„Åï„Å®„Çå„Å°„Å´Âïè„ÅÑÂêà„Çè„Åõ„Å¶„Åè„Å†„Åï„ÅÑ",
        Duration = 5
    })
    LocalPlayer:Kick("„Ç¢„ÇØ„Çª„Çπ„ÅåÊãíÂê¶„Åï„Çå„Åæ„Åó„Åü\n„Åï„Å®„Çå„Å°„Å´Âïè„ÅÑÂêà„Çè„Åõ„Å¶„Åè„Å†„Åï„ÅÑ")
    return
end

-- „Ç≠„Éº„ÇíÂèñÂæó
local REQUIRED_KEY = ALLOWED_KEYS[LocalPlayer.UserId]

-- UIÁîüÊàêÈñ¢Êï∞Ôºà„ÉÜ„É¨„Éù„Éº„ÉàÔºãLavaÂâäÈô§Ôºâ
local function createMainUI()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "TeleportGui"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = PlayerGui

    local frame = Instance.new("Frame")
    frame.Name = "TeleportFrame"
    frame.Size = UDim2.new(0, 200, 0, 100)
    frame.Position = UDim2.new(0.4, 0, 0.3, 0)
    frame.BackgroundColor3 = Color3.fromRGB(70, 130, 180)
    frame.BorderSizePixel = 0
    frame.Active = true
    frame.Parent = screenGui

    local header = Instance.new("TextLabel")
    header.Size = UDim2.new(1, 0, 0, 30)
    header.BackgroundColor3 = Color3.fromRGB(50, 90, 140)
    header.Text = "„ÉÜ„É¨„Éù„Éº„Éà"
    header.TextColor3 = Color3.new(1, 1, 1)
    header.Font = Enum.Font.SourceSansBold
    header.TextSize = 20
    header.Active = true
    header.Parent = frame

    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0.9, 0, 0.4, 0)
    button.Position = UDim2.new(0.05, 0, 0.5, 0)
    button.Text = "„Ç∏„É£„É≥„Éó„Åô„Çã"
    button.Font = Enum.Font.SourceSansBold
    button.TextSize = 20
    button.TextColor3 = Color3.new(1, 1, 1)
    button.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
    button.Parent = frame

    local destination = Vector3.new(922.8, 250.3, 32858.8)
    button.Activated:Connect(function()
        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local root = char:WaitForChild("HumanoidRootPart")
        root.CFrame = CFrame.new(destination)
    end)

    local dragging = false
    local dragStart, startPos, dragInput
    local UserInputService = game:GetService("UserInputService")

    header.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)

    header.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)

    UserInputService.InputChanged:Connect(function(input)
        if dragging and input == dragInput then
            local delta = input.Position - dragStart
            frame.Position = UDim2.new(
                startPos.X.Scale, startPos.X.Offset + delta.X,
                startPos.Y.Scale, startPos.Y.Offset + delta.Y
            )
        end
    end)

    -- LavaÈô§Âéª + ÁÑ°ÂäπÂåñ
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA("BasePart") and obj.Name:lower():find("lava") then
            obj:Destroy()
        end
    end

    workspace.DescendantAdded:Connect(function(desc)
        if desc:IsA("BasePart") and desc.Name:lower():find("lava") then
            desc.CanTouch = false
        end
    end)
end

-- KeyÂÖ•ÂäõGUIË°®Á§∫
local keyPrompt = Instance.new("ScreenGui")
keyPrompt.Name = "KeyPrompt"
keyPrompt.Parent = PlayerGui

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 360, 0, 180)
frame.Position = UDim2.new(0.5, -180, 0.5, -90)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
frame.BackgroundTransparency = 0.2
frame.AnchorPoint = Vector2.new(0.5, 0.5)
frame.Parent = keyPrompt
frame.Active = true
frame.Draggable = true

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = frame

local shadow = Instance.new("ImageLabel")
shadow.BackgroundTransparency = 1
shadow.Image = "rbxassetid://1316045217"
shadow.ImageTransparency = 0.5
shadow.ScaleType = Enum.ScaleType.Slice
shadow.SliceCenter = Rect.new(10, 10, 118, 118)
shadow.Size = UDim2.new(1, 60, 1, 60)
shadow.Position = UDim2.new(0.5, -30, 0.5, -30)
shadow.ZIndex = 0
shadow.Parent = frame

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 50)
title.Text = "üîê Key Authentication"
title.Font = Enum.Font.GothamBold
title.TextColor3 = Color3.new(1, 1, 1)
title.BackgroundTransparency = 1
title.TextScaled = true
title.Parent = frame

local inputBox = Instance.new("TextBox")
inputBox.Size = UDim2.new(0.8, 0, 0, 40)
inputBox.Position = UDim2.new(0.1, 0, 0.4, 0)
inputBox.PlaceholderText = "Enter your Key"
inputBox.TextScaled = true
inputBox.Font = Enum.Font.Gotham
inputBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
inputBox.TextColor3 = Color3.new(1, 1, 1)
inputBox.ClearTextOnFocus = false
inputBox.Text = ""
inputBox.Parent = frame

local inputCorner = Instance.new("UICorner")
inputCorner.CornerRadius = UDim.new(0, 8)
inputCorner.Parent = inputBox

local messageLabel = Instance.new("TextLabel")
messageLabel.Size = UDim2.new(1, -20, 0, 30)
messageLabel.Position = UDim2.new(0, 10, 0.8, 0)
messageLabel.BackgroundTransparency = 1
messageLabel.Text = ""
messageLabel.Font = Enum.Font.Gotham
messageLabel.TextColor3 = Color3.fromRGB(255, 80, 80)
messageLabel.TextScaled = true
messageLabel.Visible = false
messageLabel.Parent = frame

inputBox.FocusLost:Connect(function()
    if inputBox.Text == REQUIRED_KEY then
        keyPrompt:Destroy()
        createMainUI()
    else
        messageLabel.Text = "Invalid Key, try again."
        messageLabel.Visible = true
        inputBox.Text = ""
    end
end)
